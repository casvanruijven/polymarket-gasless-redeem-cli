{
  "name": "polymarket-gasless-redeem-cli",
  "version": "2.0.0",
  "description": "Gasless redemption CLI tool for Polymarket positions",
  "main": "dist/redeem.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "setup": "tsx src/redeem.ts --setup",
    "reset": "tsx src/redeem.ts --reset",
    "check": "tsx src/redeem.ts --check",
    "redeem": "tsx src/redeem.ts",
    "start": "tsx src/redeem.ts",
    "dev": "tsx watch src/redeem.ts"
  },
  "dependencies": {
    "@polymarket/builder-relayer-client": "^0.0.8",
    "@polymarket/builder-signing-sdk": "^0.0.8",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
